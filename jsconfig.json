{
  "baseUrl": ".",
  "include": ["src/**/*", "examples"],
  "exclude": ["node_modules", "dist"],
  "typeAcquisition": { "exclude": ["dotenv", "source-map"] },
  "compilerOptions": {
    "baseUrl": ".",
    "target": "esnext",
    "module": "es2015",
    "paths": {
      "@/*": ["src/*"],
      "@": [
        "src/index.js",
        "src/index.json",
        "src/index.vue",
        "src/index.scss",
        "src/index.css"
      ],
      "@assets/*": ["src/assets/*"],
      "@assets": [
        "src/assets/index.js",
        "src/assets/index.json",
        "src/assets/index.vue",
        "src/assets/index.scss",
        "src/assets/index.css"
      ],
      "@components/*": ["src/components/*"],
      "@components": [
        "src/components/index.js",
        "src/components/index.json",
        "src/components/index.vue",
        "src/components/index.scss",
        "src/components/index.css"
      ],
      "@common/*": ["src/components/common/*"],
      "@common": [
        "src/components/common/index.js",
        "src/components/common/index.json",
        "src/components/common/index.vue",
        "src/components/common/index.scss",
        "src/components/common/index.css"
      ],
      "@uncommon/*": ["src/components/uncommon/*"],
      "@uncommon": [
        "src/components/uncommon/index.js",
        "src/components/uncommon/index.json",
        "src/components/uncommon/index.vue",
        "src/components/uncommon/index.scss",
        "src/components/uncommon/index.css"
      ],
      "@locales/*": ["src/locales/*"],
      "@locales": [
        "src/locales/index.js",
        "src/locales/index.json",
        "src/locales/index.vue",
        "src/locales/index.scss",
        "src/locales/index.css"
      ],
      "@pages/*": ["src/pages/*"],
      "@pages": [
        "src/pages/index.js",
        "src/pages/index.json",
        "src/pages/index.vue",
        "src/pages/index.scss",
        "src/pages/index.css"
      ],
      "@router/*": ["src/router/*"],
      "@router": [
        "src/router/index.js",
        "src/router/index.json",
        "src/router/index.vue",
        "src/router/index.scss",
        "src/router/index.css"
      ],
      "@store/*": ["src/store/*"],
      "@store": [
        "src/store/index.js",
        "src/store/index.json",
        "src/store/index.vue",
        "src/store/index.scss",
        "src/store/index.css"
      ],
      "@use/*": ["src/use/*"],
      "@use": [
        "src/use/index.js",
        "src/use/index.json",
        "src/use/index.vue",
        "src/use/index.scss",
        "src/use/index.css"
      ],
      "@examples/*": ["examples/*"],
      "@examples": [
        "examples/index.js",
        "examples/index.json",
        "examples/index.vue",
        "examples/index.scss",
        "examples/index.css"
      ],
      "@core/*": ["src/core/*"],
      "@core": [
        "src/core/index.js",
        "src/core/index.json",
        "src/core/index.vue",
        "src/core/index.scss",
        "src/core/index.css"
      ],
      "@theme/*": ["src/core/components/theme/*"],
      "@theme": [
        "src/core/components/theme/index.js",
        "src/core/components/theme/index.json",
        "src/core/components/theme/index.vue",
        "src/core/components/theme/index.scss",
        "src/core/components/theme/index.css"
      ],
      "@constants/*": ["src/core/constants/*"],
      "@constants": [
        "src/core/constants/index.js",
        "src/core/constants/index.json",
        "src/core/constants/index.vue",
        "src/core/constants/index.scss",
        "src/core/constants/index.css"
      ],
      "@layouts/*": ["src/core/layouts/*"],
      "@layouts": [
        "src/core/layouts/index.js",
        "src/core/layouts/index.json",
        "src/core/layouts/index.vue",
        "src/core/layouts/index.scss",
        "src/core/layouts/index.css"
      ],
      "@middleware/*": ["src/core/middleware/*"],
      "@middleware": [
        "src/core/middleware/index.js",
        "src/core/middleware/index.json",
        "src/core/middleware/index.vue",
        "src/core/middleware/index.scss",
        "src/core/middleware/index.css"
      ],
      "@mixins/*": ["src/core/mixins/*"],
      "@mixins": [
        "src/core/mixins/index.js",
        "src/core/mixins/index.json",
        "src/core/mixins/index.vue",
        "src/core/mixins/index.scss",
        "src/core/mixins/index.css"
      ],
      "@plugins/*": ["src/core/plugins/*"],
      "@plugins": [
        "src/core/plugins/index.js",
        "src/core/plugins/index.json",
        "src/core/plugins/index.vue",
        "src/core/plugins/index.scss",
        "src/core/plugins/index.css"
      ],
      "@services/*": ["src/core/services/*"],
      "@services": [
        "src/core/services/index.js",
        "src/core/services/index.json",
        "src/core/services/index.vue",
        "src/core/services/index.scss",
        "src/core/services/index.css"
      ],
      "@utils/*": ["src/core/utils/*"],
      "@utils": [
        "src/core/utils/index.js",
        "src/core/utils/index.json",
        "src/core/utils/index.vue",
        "src/core/utils/index.scss",
        "src/core/utils/index.css"
      ]
    }
  }
}
